    agent any
  
    stages {
    
      stage("build"){
          steps {
              echo 'Hello my pipeline'
          }
      }    
      stage("test"){
          
          steps{
               echo 'Hello my test pipeline'
          }
          
      }
      stage("deploy"){
           
          steps{
              echo 'Hello my deploy pipeline'
          }
      }
    }
}
   
